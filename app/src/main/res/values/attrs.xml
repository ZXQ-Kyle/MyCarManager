<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ImgAndEtView">
        <attr name="drawableColor" format="color|reference"/>
        <attr name="iconLeft" format="reference"/>
        <attr name="hint" format="string"/>
        <attr name="text" format="string"/>
        <attr name="unEditable" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ClearEditText">
        <!-- 清除按钮的图标 -->
        <attr name="iconClear" format="reference"/>
        <!--icon混色-->
        <attr name="iconTint" format="color"/>
    </declare-styleable>
</resources>